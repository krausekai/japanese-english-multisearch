{
	"name": "JapaneseEnglishMultisearch",
	"productName": "Japanese-English MultiSearch",
	"description": "MultiSearch dictionaries and corpora",
	"version": "0.1.0",
	"author": "Kai Krause <kaikrause95@gmail.com>",
	"copyright": "Â© 2017, Kai Krause <kaikrause95@gmail.com>",
	"homepage": "http://example.com",
	"license": "MIT",
	"repository": "https://github.com/krausekai/japanese-english-multisearch",
	"keywords": [
		"Japanese",
		"English",
		"MultiSearch"
	],
	"main": "main.js",
	"build": {
		"appId": "com.kaikrause.multisearch",
		"asarUnpack": [
			"./assets/"
		],
		"publish": [
			{
				"provider": "github",
				"owner": "krausekai",
				"repo": "japanese-english-multisearch"
			}
		],
		"win": {
			"target": "nsis"
		},
		"nsis": {
			"oneClick": false,
			"perMachine": false
		},
		"linux": {
			"target": "AppImage",
			"category": "Education"
		},
		"mac": {
			"target": "dmg"
		}
	},
	"scripts": {
		"start": "electron main.js",
		"postinstall": "install-app-deps",
		"pack": "build --dir --ia32",
		"dist:win": "build --win --ia32",
		"dist:linux": "build --linux --x64",
		"dist:mac": "build --mac --x64"
	},
	"devDependencies": {
		"electron": "latest",
		"electron-builder": "latest"
	},
	"dependencies": {
		"cheerio": "latest",
		"electron-context-menu": "latest",
		"electron-updater": "latest",
		"request": "latest"
	}
}
